{
  "name": "statusbarcolorizer",
  "displayName": "statusbarColorizer",
  "description": "color the status bar based on the folder name",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.statusbarcolorizer"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.statusbarcolorizer",
        "title": "Status Bar Colorizer"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "eslint": "^5.13.0",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "color-hash": "^1.0.3",
    "tinycolor2": "^1.4.1"
  }
}
